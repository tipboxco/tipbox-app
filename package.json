{
  "name": "tipbox-app",
  "version": "1.0.0",
  "main": "index.ts",
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "scripts": {
    "start": "expo start",
    "start:dev": "copy .env.development .env && expo start",
    "start:test": "copy .env.test .env && expo start",
    "start:prod": "copy .env.production .env && expo start",
    "android": "copy .env.development .env && set DARK_MODE=media && expo start --android",
    "android:test": "copy .env.test .env && set DARK_MODE=media && expo start --android",
    "android:prod": "copy .env.production .env && set DARK_MODE=media && expo start --android",
    "ios": "copy .env.development .env && set DARK_MODE=media && expo start --ios",
    "ios:test": "copy .env.test .env && set DARK_MODE=media && expo start --ios",
    "ios:prod": "copy .env.production .env && set DARK_MODE=media && expo start --ios",
    "web": "copy .env.development .env && set DARK_MODE=media && expo start --web",
    "web:test": "copy .env.test .env && set DARK_MODE=media && expo start --web",
    "web:prod": "copy .env.production .env && set DARK_MODE=media && expo start --web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@gluestack-ui/button": "^1.0.14",
    "@gluestack-ui/icon": "^0.1.27",
    "@gluestack-ui/nativewind-utils": "^1.0.26",
    "@gluestack-ui/overlay": "^0.1.22",
    "@gluestack-ui/toast": "^1.0.9",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/drawer": "^7.5.3",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.21",
    "@react-navigation/stack": "^7.4.2",
    "axios": "^1.10.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "expo": "~53.0.17",
    "expo-constants": "^17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-device": "^7.1.4",
    "expo-notifications": "^0.31.4",
    "expo-secure-store": "^14.2.3",
    "expo-status-bar": "~2.2.3",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-css-interop": "^0.1.22",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "tailwindcss": "^3.4.17",
    "zustand": "^5.0.6",
    "expo-updates": "~0.28.17"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "jscodeshift": "^0.15.2",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3"
  },
  "private": true
}
