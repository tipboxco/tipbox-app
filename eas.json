{
  "cli": {
    "version": ">= 12.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "prebuildCommand": "copy .env.development.txt .env",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "medium"
      },
      "env": {
        "EXPO_PUBLIC_ENV": "development"
      }
    },
    "test": {
      "developmentClient": false,
      "distribution": "internal",
      "prebuildCommand": "copy .env.test.txt .env",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "medium"
      },
      "env": {
        "EXPO_PUBLIC_ENV": "test"
      }
    },
    "production": {
      "developmentClient": false,
      "distribution": "store",
      "prebuildCommand": "copy .env.production.txt .env",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "medium"
      },
      "env": {
        "EXPO_PUBLIC_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
} 